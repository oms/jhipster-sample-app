pipeline {
  agent any
  stages {
    stage('Build Backend') {
      steps {
        echo 'Building'
      }
    }
    stage('Test Backend') {
      steps {
        echo 'Testing'
      }
    }
    stage('More Tests') {
      steps {
        echo 'Moar tests'
      }
    }
    stage('Deploy Staging') {
      steps {
        echo 'Deploy staging'
      }
    }
    stage('Deploy Production') {
      steps {
        echo 'Deploy prod'
      }
    }
  }
}